# Notion API Configuration
NOTION_TOKEN=your_notion_integration_token_here
NOTION_VERSION=2022-06-28
NOTION_RATE_LIMIT_REQUESTS=3
NOTION_RATE_LIMIT_WINDOW=1000

# Obsidian Vault Configuration  
OBSIDIAN_VAULT_PATH=/path/to/your/obsidian/vault
OBSIDIAN_ATTACHMENTS_FOLDER=attachments
OBSIDIAN_TEMPLATE_FOLDER=templates
OBSIDIAN_PRESERVE_STRUCTURE=true
OBSIDIAN_CONVERT_IMAGES=true
OBSIDIAN_CONVERT_DATABASES=true

# Import Settings
BATCH_SIZE=50
CONCURRENCY=3
RETRY_ATTEMPTS=3
PROGRESS_TRACKING=true

# Logging Configuration
LOG_LEVEL=info
LOG_DIR=./logs
LOG_FILE=importer.log

# Development Settings
NODE_ENV=development