{
  "name": "notion-obsidian-importer",
  "version": "1.0.0",
  "description": "A robust TypeScript tool for importing Notion workspaces to Obsidian with progressive download and conversion",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "notion-obsidian-importer": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:plugin": "tsc && cp manifest.json dist/",
    "dev": "ts-node src/index.ts",
    "dev:cli": "ts-node src/cli/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.15",
    "axios": "^1.6.2",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.3.8",
    "ora": "^5.4.1",
    "p-queue": "^8.0.1",
    "p-retry": "^6.2.0",
    "turndown": "^7.1.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.9.0",
    "@types/turndown": "^5.0.4",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "obsidian": "^1.8.7",
    "prettier": "^3.1.0",
    "ts-jest": "^29.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "notion",
    "obsidian",
    "importer",
    "markdown",
    "converter",
    "typescript"
  ],
  "author": "Backend Developer",
  "license": "MIT"
}
